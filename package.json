{
  "name": "tape-testcheck",
  "version": "0.1.1",
  "description": "Use Testcheck with Tape test runner",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel ./src/index.js --out-file ./dist/index.js",
    "test": "babel-node ./node_modules/.bin/tape ./test/index.js"
  },
  "keywords": [
    "tape",
    "testcheck"
  ],
  "author": "npbee",
  "license": "MIT",
  "dependencies": {
    "tape": "^4.2.1",
    "testcheck": "^0.1.4"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "sinon": "^1.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npbee/tape-testcheck"
  }
}
